# WebSocket MCP Proxy Environment Variables
# Copy this file to .env and fill in your actual API keys and tokens

# Vapi AI Voice Platform
VAPI_TOKEN=your_vapi_token_here

# Supabase Database
SUPABASE_ACCESS_TOKEN=your_supabase_access_token_here
SUPABASE_PROJECT_REF=your_supabase_project_ref_here

# N8N Railway Integration
N8N_RAILWAY_TOKEN=your_n8n_railway_token_here

# Linear Project Management
LINEAR_API_KEY=your_linear_api_key_here

# Railway Platform
RAILWAY_API_KEY=your_railway_api_key_here

# Cartesia AI Voice
CARTESIA_API_KEY=your_cartesia_api_key_here

# Firecrawl Web Scraping
FIRECRAWL_API_KEY=your_firecrawl_api_key_here

# Tavily Search
TAVILY_API_KEY=your_tavily_api_key_here

# OpenRouter AI Models
OPENROUTER_API_KEY=your_openrouter_api_key_here

# GitHub Integration
GITHUB_API_KEY=your_github_api_key_here

# HeyGen Video Generation
HEYGEN_API_KEY=your_heygen_api_key_here

# Mem0 Memory Management
DATABASE_URL=postgresql://localhost:5432/mem0
OPENAI_API_KEY=your_openai_api_key_here

# OpenMemory
OPENMEMORY_API_KEY=your_openmemory_api_key_here

# HTTP/SSE/STREAMABLE_HTTP Servers (for future implementation)
# These require additional proxy functionality not yet implemented:

# Pipedream MCP Cloud (SSE)
PIPEDREAM_MCP_URL=https://mcp.pipedream.net/v2

# Postman API Server (STREAMABLE_HTTP)
POSTMAN_MCP_URL=https://mcp.postman.com/mcp
POSTMAN_BEARER_TOKEN=your_postman_bearer_token_here

# MC3 Server Endpoint (STREAMABLE_HTTP)
MC3_SERVER_URL=https://mcp.hitsdifferent.ai/metamcp/mc3-server/mcp
MC3_BEARER_TOKEN=your_mc3_bearer_token_here

# Rube (STREAMABLE_HTTP)
RUBE_MCP_URL=https://rube.app/mcp
RUBE_BEARER_TOKEN=your_rube_bearer_token_here
